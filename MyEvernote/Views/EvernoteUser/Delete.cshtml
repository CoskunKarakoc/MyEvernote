            <dd class="text-info">
                @Html.DisplayFor(model => model.ModifiedOn)
            </dd>

            <dt class="text-info">
                @Html.DisplayNameFor(model => model.ModifiedUsername)
            </dt>

            <dd class="text-info">
                @Html.DisplayFor(model => model.ModifiedUsername)
            </dd>

        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <button class="btn btn-default btn-sm" type="submit"><span class="glyphicon glyphicon-trash"></span> Delete</button> |
                <a href="@Url.Action("Index","EvernoteUser")" class="btn btn-default btn-sm"><span class="glyphicon glyphicon-hand-left"></span> Back to List</a>
            </div>
        }
    </div>
</div>
